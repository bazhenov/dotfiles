PreferredAuthentications "publickey,password"
GSSAPIAuthentication no
PubkeyAcceptedKeyTypes=+ssh-dss

Host *
	UseKeychain yes

Host pstorage
	Hostname 109.126.16.96
	User bazhenov
	Port 2022

Host pdesktop
	User bazhenov
	Hostname 109.126.16.96
	Port 2122
	DynamicForward 3128
	ForwardAgent yes
	LogLevel QUIET
	RemoteCommand tmux attach || tmux new
	RequestTTY force

Host *.srv.loc
	IdentityFile ~/.ssh/vhost-dsa
	HostKeyAlgorithms +ssh-dss

Host artifacts-proxy.akod.loc
	DynamicForward 31281
	HostName artifacts.akod.loc
	LogLevel QUIET
	
Host *.akod.loc
	IdentityFile ~/.ssh/vhost-dsa
	HostKeyAlgorithms +ssh-dss
	User tech
	ProxyCommand ssh -e none tunnel@aux-msk.srv.loc %h %p
	PreferredAuthentications publickey

Host artifacts.bazadev.net
	HostKeyAlgorithms +ssh-dss
	PreferredAuthentications publickey
	IdentityFile ~/.ssh/vhost-dsa
	User tech

Host vault.bazadev.net
	User logs

Host *.bazadev.net
	User tech
	IdentityFile ~/.ssh/vhost-dsa

Host *.bazadev.private
	IdentityFile ~/.ssh/vhost-dsa

