[color]
	diff = auto
	status = auto
	branch = auto
[alias]
	ci = commit
	co = checkout
	br = branch
	st = status
	df = diff
[user]
	name = Denis Bazhenov
	email = bazhenov@farpost.com
[branch]
	autosetupmerge = true
	autosetuprebase = always
[core]
	autocrlf = input
	excludesfile = /Users/bazhenov/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = current
